<?xml version="1.0"?>
<configuration>
    <connectionStrings>
        <add name="AccessConnection" connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Data Source=DB\Database1.accdb"
            providerName="System.Data.OleDb" />
    </connectionStrings>
    <system.web>
      <compilation debug="true" strict="false" explicit="true" targetFramework="4.0" />
    </system.web>
  <appSettings>
    <add key="selectCommand" value="SELECT * FROM [DATA_DICTIONARY] WHERE [TABLE_NAME] = @Table ORDER BY [KEY] DESC, [COLUMN_NAME] ASC"/>
    <add key="deleteCommand" value="DELETE FROM [DATA_DICTIONARY] WHERE ID = ?"/>
    <add key="insertCommand" value="INSERT INTO [DATA_DICTIONARY] ([TABLE_NAME], [COLUMN_NAME], [COLUMN_TYPE], [COLUMN_SIZE], [PRECISION], [SCALE], [NULLABILITY], [KEY], [DESCRIPTION]) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?);"/>
    <add key="updateCommand" value="UPDATE [DATA_DICTIONARY] SET [TABLE_NAME] = ?, [COLUMN_NAME] = ?, [COLUMN_TYPE] = ?, [COLUMN_SIZE] = ?, [PRECISION] = ?, [SCALE] = ?, [NULLABILITY] = ?, [KEY] = ?, [DESCRIPTION] = ? WHERE [ID] = ?"/>
    <add key="searchWCommand" value="SELECT * FROM [DATA_DICTIONARY] WHERE [COLUMN_NAME] LIKE '%' + @search + '%' AND [TABLE_NAME] LIKE '%' + @table + '%' ORDER BY [KEY], [COLUMN_NAME]" />
    <add key="searchCommand" value="SELECT * FROM [DATA_DICTIONARY] WHERE [COLUMN_NAME] LIKE @search AND [TABLE_NAME] LIKE @table" />
    <add key="DataTypes" value="char(n) varchar(n) varchar(max) text nchar nvarchar nvarchar(max) ntext bit binary(n) varbinary varbinary(max) image tinyint smallint int bigint decimal(p,s) numeric(p,s) smallmoney money float(n) real datetime datetime2 smalldatetime date time datetimeoffset timestamp sql_variant uniqueidentifier xml cursor table"/>
  </appSettings>

</configuration>
